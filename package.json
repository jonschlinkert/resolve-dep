  "homepage": "https://github.com/jonschlinkert/resolve-dep",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Bogdan Chadkin <trysound@yandex.ru> (https://github.com/TrySound)",
