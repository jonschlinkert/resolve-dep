    "Charlike Mike Reagent (http://i.am.charlike.online)",
    "Jarid Margolin <jaridmargolin@gmail.com> (https://github.com/jaridmargolin)",
    "Jon Schlinkert <jon.schlinkert@sellside.com> (http://twitter.com/jonschlinkert)"
  ],
  "repository": "jonschlinkert/resolve-dep",
  "bugs": {
    "url": "https://github.com/jonschlinkert/resolve-dep/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">= 0.10.16"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arrayify-compact": "^0.2.0",
    "cwd": "^0.10.0",
    "extend-shallow": "^2.0.1",
